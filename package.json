{
  "name": "unifi-access-api",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "generate:js": "npm run clean && protoc -I=./proto -I=./third_party/api-common-protos --js_out=import_style=commonjs,binary:dist ./proto/*.proto",
    "generate:ts": "npm run clean && protoc -I=./proto -I=./third_party/api-common-protos --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=dist ./proto/*.proto",
    "generate:python": "npm run clean && protoc -I=./proto -I=./third_party/api-common-protos --python_out=dist ./proto/*.proto",
    "generate:html": "protoc -I=./proto -I=./third_party/api-common-protos --doc_out=./docs --doc_opt=html,index.html ./proto/*.proto"
  },
  "dependencies": {
    "protoc-gen-js": "^3.21.2",
    "protoc-gen-ts": "^0.8.7",
    "ts-protoc-gen": "^0.15.0"
  },
  "devDependencies": {
    "protolint": "^0.50.5"
  }
}
